{
  "name": "rsms2",
  "version": "0.2.2",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@arrowood.dev/socket": "^0.2.0",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.1",
    "@mdi/font": "7.0.96",
    "@vuepic/vue-datepicker": "^7.4.1",
    "core-js": "^3.34.0",
    "echarts": "^5.4.3",
    "electron-edge-js": "github:cloudpsn1993/electron-edge-js",
    "electron-updater": "^6.1.7",
    "moment": "^2.30.1",
    "mysql2": "^3.6.5",
    "pinia": "^2.1.7",
    "roboto-fontface": "*",
    "vee-validate": "^4.12.4",
    "vite-raw-plugin": "^1.0.2",
    "vue-echarts": "^6.6.8",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.5",
    "vue3-apexcharts": "^1.4.4",
    "vue3-perfect-scrollbar": "^1.6.1",
    "vuetify": "^3.4.7"
  },
  "devDependencies": {
    "@babel/types": "^7.23.6",
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.6.1",
    "@types/node": "^20.10.5",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/ui": "^1.0.4",
    "@vue/devtools": "^7.0.13",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0-beta.2",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-fonts": "^1.0.3",
    "vite": "^5.0.10",
    "vite-plugin-vuetify": "^2.0.1",
    "vitest": "^1.0.4",
    "vue": "^3.3.12",
    "vue-tsc": "^1.8.25"
  },
  "build": {
    "productName": "RSMS2",
    "appId": "scu.RSMS.app",
    "copyright": "Copyright [2023] by [College Of Physics,SiChuan University]",
    "compression": "store",
    "directories": {
      "output": "dist"
    },
    "files": [
      "!**/.vscode/*",
      "!.vscode",
      "!assets",
      "!plugins",
      "!src",
      "!tmp",
      "!electron.vite.config.{js,ts,mjs,cjs}",
      "!{.eslintignore,.eslintrc.js,.eslintrc.cjs,webpack.config,.prettierignore,.prettierrc.yaml,dev-app-update.yml,CHANGELOG.md,README.md}",
      "!{.env,.env.*,.npmrc,pnpm-lock.yaml}",
      "!{tsconfig.json,tsconfig.node.json,tsconfig.web.json}"
    ],
    "asarUnpack": [],
    "extraResources": [
      {
        "from": "./plugins",
        "to": "../plugins",
        "filter": [
          "**/*",
          "!.idea"
        ]
      }
    ],
    "asar": {
      "smartUnpack": false
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
